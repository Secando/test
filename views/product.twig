{% extends 'app.twig' %}
<title>{% block title %}Продукт{% endblock %}</title>
{% block content %}

    <div id="main" style="margin-top: 25px">
        <div id="home_head">
            <div id="product_info">
                <div style="width: 330px; height: 480px ; display: flex; align-items: center;justify-content: center;border: 1px solid;border-radius: 5px">
                    <img src="http://localhost:8000/assets/images/{{ product.imagePath }}" width="300px" height="440">
                </div>
                <div style="display: flex; flex-direction: column; align-items: start; height: 480px; margin-left: 30px">
                    <div id="product_name_info">{{ product.englishName }}<br>{{ product.rusName }}</div>

                    <div>Тип: Манга</div>
                    <div>Количество страниц: {{ product.pageCount }}</div>
                    <div>Автор: {{ product.author }}</div>
                    <div>Издательство: Shuiesha</div>
                </div>
                <div id="product_price">
                    <div style="display: flex;justify-content: space-between;">
                        <div style="font-weight: bold; font-size: 140%;margin-bottom: 8px">{{ product.cost }}₽</div>
                        <form method="post" action="/product/favorite">
                            <input type="hidden" name="id" value="{{ product.id }}">
                            {% if isLogin %}
                            <button style="width: 40px; height: 30px; border: none; background: white; margin-top: 5px">
                                <svg id="add_to_favorite" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"
                                     width="26"
                                     style="margin-bottom: 6px;margin-right: 4px">
                                    <!--!Font Awesome Free 6.5.2 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2024 Fonticons, Inc.-->
                                    <path {% if inFavorite %} fill="red" {% endif%} d="M225.8 468.2l-2.5-2.3L48.1 303.2C17.4 274.7 0 234.7 0 192.8v-3.3c0-70.4 50-130.8 119.2-144C158.6 37.9 198.9 47 231 69.6c9 6.4 17.4 13.8 25 22.3c4.2-4.8 8.7-9.2 13.5-13.3c3.7-3.2 7.5-6.2 11.5-9c0 0 0 0 0 0C313.1 47 353.4 37.9 392.8 45.4C462 58.6 512 119.1 512 189.5v3.3c0 41.9-17.4 81.9-48.1 110.4L288.7 465.9l-2.5 2.3c-8.2 7.6-19 11.9-30.2 11.9s-22-4.2-30.2-11.9zM239.1 145c-.4-.3-.7-.7-1-1.1l-17.8-20c0 0-.1-.1-.1-.1c0 0 0 0 0 0c-23.1-25.9-58-37.7-92-31.2C81.6 101.5 48 142.1 48 189.5v3.3c0 28.5 11.9 55.8 32.8 75.2L256 430.7 431.2 268c20.9-19.4 32.8-46.7 32.8-75.2v-3.3c0-47.3-33.6-88-80.1-96.9c-34-6.5-69 5.4-92 31.2c0 0 0 0-.1 .1s0 0-.1 .1l-17.8 20c-.3 .4-.7 .7-1 1.1c-4.5 4.5-10.6 7-16.9 7s-12.4-2.5-16.9-7z"/>
                            </button>
                            </svg>
                            {% endif %}
                        </form>
                    </div>

                    <form method="post" action="/catalog/cart" style="width: 250px">
                        <input hidden="hidden" name="id" value="{{ product.id }}">
                        {% if inCart %}
                            <div>В корзине</div>
                            <button id="add_to_cart" type="submit"
                                    style="background-color: orange; width: 240px; height: 35px; padding-top: 6px;border-radius: 5px;margin-bottom: 10px">
                                <div style="margin-left: 8px; font-family: 'Source Code Pro'; font-size: 120%;margin-bottom: 5px">
                                    Добавить ещё
                                </div>
                            </button>

                            <a href="/cart">
                                <button id="add_to_cart"
                                        style="background-color: deepskyblue; width: 240px; height: 35px; padding-top: 6px;border-radius: 5px;margin-bottom: 10px">
                                    <div style="margin-left: 8px; font-family: 'Source Code Pro'; font-size: 120%;margin-bottom: 5px">
                                        Перейти в коризну
                                    </div>
                                </button>
                            </a>

                        {% else %}
                            {% if isLogin %}
                            <button id="add_to_cart"
                                    style="background-color: orange; width: 240px; height: 35px; padding-top: 6px;border-radius: 5px;margin-bottom: 10px">
                                <div style="margin-left: 8px; font-family: 'Source Code Pro'; font-size: 120%;margin-bottom: 5px">

                                    Добавить в корзину
                                </div>
                            </button>
                                {% else %}
                                <a href="/login">Войдите в личный кабинет для покупки</a>
                                    {% endif %}
                        {% endif %}
                    </form>
                    <div style="border-top: 1px solid;font-size: 90%; padding-top: 10px">Срок доставки:<br>от 14 дней
                    </div>
                </div>

            </div>
            <div id="comments" style="width: 800px;margin-left: 150px">
                <div style="margin-bottom: 20px">Отзывы о товаре</div>
